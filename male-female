#include<stdio.h> 
int main() 
{ 
	char ch; 
	printf(" Enter a character(M/F): "); 
	scanf("%c",&ch); 
	switch(ch) 
	{ 
	case 'm': 
	case 'M': 
			printf(" ------------ MALE ------------"); 
			break; 
	case 'f': 
	case 'F':
			printf(" ------------ FEMALE ------------"); 
			break; 
	} 
	printf("\n"); 
}
